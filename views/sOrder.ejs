    <title>Document</title>
    <style>
        .orderProductItemList {
            margin: 0;
        }
        ul{
            margin-bottom: 0;
        }
        .titleHead {
            background: var(--dYellow);
            color: white;
            height: 40px;
            margin-bottom: 8px;
            position: relative;
            padding-left: 32px;
            padding-right: 32px;
        }
        .titleHead>div:not(:first-child),
        .orderList li>div:not(:first-child){
            padding-left: 32px;
        }        
        .orderList li {
            padding: 14px 32px;
            border: 1px solid var(--moreLightGray);
            border-radius: 20px;
            
        }


        .orderList li+li{
            margin-bottom: 8px;
        }
       
        .orderList img {
            width: 60px;
            height: 60px;
        }

        .orderList{
            max-height: calc(100vh - 306px);
